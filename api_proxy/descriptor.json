{
    "baseUrl": "https://evzijst.bitbucket.io/api_proxy",
    "name": "Addon API Proxy Example",
    "key": "ADDON_PROXY_EXAMPLE",
    "description": "Example addon that declares an addon proxy rule.",
    "contexts": ["account"],
    "modules": {
        "proxy": {
            "/repository.json" : {
                "destination": "/repository.json?user={user.username}",
                "methods": {
                    "get": {}
                }
            }
        }
    }
}